pData[3] = arguments[1];

}


}


}
else if ((arguments.length) == 3) {
if ((typeof (arguments[0])) == "number") {
if ((typeof (arguments[1])) == "number") {
var pData2=arguments[2].pData;
pData[0] = arguments[0];
pData[1] = arguments[1];
