this._pFile.size = 0;
if (fnSuccess) {
fnSuccess.apply(this);

}


};
LocalFileSimplified.prototype._read = function() {
var pFile=this._pFile;
var pData=localStorage.getItem(this.path);
if (pData == null) {
pData = "";
if ((pData & (1 << 1)) != 0) {
localStorage.setItem(this.path, pData);

}


}

