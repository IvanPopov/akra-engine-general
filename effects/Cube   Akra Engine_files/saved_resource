pPixel[1] = ((((2 * (pColor1 & 2016)) >>> 5) * 4) + (((pColor0 & 2016) >>> 5) * 4)) / 3;
pPixel[2] = (((2 * (pColor1 & 31)) * 8) + ((pColor0 & 31) * 8)) / 3;

}





}
















return pPixel;

};
Img.prototype.convert = function(eFormat) {
var iLength;
var iOffset;
var iColor1;
var iColor2;
var iColor3;
var iColor4;
var pOldBuffer;
var pNewBuffer;
var pNewData;
var pPixel=new Uint8Array(4);
var nMipMap=this.getMipLevels();
