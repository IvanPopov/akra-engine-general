this._pDevice.bindBuffer(34962, this._pBuffer);

};
VertexBuffer.prototype.unbind = function() {
trace("!!!!! VertexBuffer.prototype.unbind #" + (this.resourceHandle()));
this._pDevice.bindBuffer(34962, null);

};
