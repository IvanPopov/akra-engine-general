if (this.constructor.pBaseFunctionsHash[sHash]) {
if (!0) {
var err=((((((("Error:: " + "bad 193") + "\n") + "\tfile: ") + "C:/webservers/home/akra/www/akra-engine-core/src/shaders/Effect.js") + "\n") + "\tline: ") + "") + "\n";
