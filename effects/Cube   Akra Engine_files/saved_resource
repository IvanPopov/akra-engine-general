this._iWidth = 0;
this._iHeight = 0;
this._eFormat = 6408;
this._eType = 5121;
this._iSlot = -1;
this._pEngine = pEngine;
this._pRepackTexture = null;
this._pSystemVertexDataTexture = null;
this._isTextureChanged = false;
this._initSystemStorageTexture(pEngine);

}

