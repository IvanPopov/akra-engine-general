console.log((((("[DEBUG][" + "C:/webservers/home/akra/www/akra-engine-core/src/resources/Texture.js") + "][") + "") + "]") + ("TODO:: " + "Texture:: resize with scale"));
throw new Error("TODO::\n" + "Texture:: resize with scale");
