isCopyAll = isCopyAll || true;
console.log((((("[DEBUG][" + "C:/webservers/home/akra/www/akra-engine-core/src/resources/Texture.js") + "][") + "") + "]") + ("TODO:: " + "Texture::uploadCubeFace()"));
