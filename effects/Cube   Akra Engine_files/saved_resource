return crc32(this);

};
String.prototype.replaceAt = function(n, chr) {
return ((this.substr(0, n)) + chr) + (this.substr(n + (chr.length)));

};
Number.prototype.toHex = function(iLength) {
"use strict";
var sValue=this.toString(16);
for (var i=0; i < (iLength - (sValue.length)); ++i) {
sValue = "0" + sValue;

}

return sValue;

};
