pColor = new Uint8Array(this._pData[iVolumeLevel][eCubeFlag][iMipLevel], iOffset, 3);
pPixel[0] = pColor[0];
pPixel[1] = pColor[1];
pPixel[2] = pColor[2];
pPixel[3] = 255;

}
else if ((this._eFormat) == (36195)) {
