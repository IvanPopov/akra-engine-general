var WEBSOCKET_PORT=1337;
if (!a) {
var a= {};

}

if (!(a.NET)) {
a.NET =  {};

}

if (!error) {
var error=function(e) {
throw new Error(e);

};

}

if (!trace) {
var trace=function() {

};

