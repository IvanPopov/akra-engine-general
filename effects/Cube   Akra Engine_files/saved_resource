this.notifyRestored();
return true;

};
VertexBuffer.prototype.disableResource = function() {
this.notifyDisabled();
if (((this._iStateFlags & (1 << a.VBufferBase.Volatile)) != 0) && ((this._iTypeFlags & (1 << 3)) != 0)) {
this._pDevice.deleteBuffer(this._pBuffer);
this._pBuffer = null;

}

return true;

};
