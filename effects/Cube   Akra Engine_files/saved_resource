this.isGlobal = ((pType.pUsagesName["global"]) === null? true : false);
this.isUniform = (this.isUniform) || (this.isSampler());

}

if (this.isPointer) {
this.iSize = 1;

}
else  {
this.iSize = (this.pType.pEffectType.iSize) * (this.iLength);

}


};
