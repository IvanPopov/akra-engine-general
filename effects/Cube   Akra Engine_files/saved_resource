if (((ppScopes[pScopeStack[i]]) && (ppScopes[pScopeStack[i]].pTypeTable)) && (ppScopes[pScopeStack[i]].pTypeTable[sTypeName])) {
return ppScopes[pScopeStack[i]].pTypeTable[sTypeName];

}


}

return false;

};
