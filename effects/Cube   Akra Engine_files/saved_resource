var i;
for (i = (pScopeStack.length) - 1; i >= 0; i--) {
if (((ppScopes[pScopeStack[i]]) && (ppScopes[pScopeStack[i]].pVariableTable)) && (ppScopes[pScopeStack[i]].pVariableTable[sName])) {
return ppScopes[pScopeStack[i]].pVariableTable[sName];

}


}

return false;

};
