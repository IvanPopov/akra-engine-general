Node.prototype.normalMatrix = function() {
if ((this._iUpdateFlags & (1 << 5)) != 0) {
this._m4fWorldMatrix.toInverseMat3(this._m3fNormalMatrix).transpose();
this._iUpdateFlags &= ~(1 << 5);

}

return this._m3fNormalMatrix;

};
